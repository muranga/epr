<h1>Listing reports</h1>

<table>
  <tr>
    <th>User</th>
    <th>Project</th>
    <th>Title</th>
    <th>Abstract</th>
    <th>File</th>
    <th>Status</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @reports.each do |report| %>
  <tr>
    <td><%= report.user_id %></td>
    <td><%= report.project_id %></td>
    <td><%= report.title %></td>
    <td><%= report.abstract %></td>
    <td><%= report.file %></td>
    <td><%= report.status %></td>
    <td><%= link_to 'Show', report %></td>
    <td><%= link_to 'Edit', edit_report_path(report) %></td>
    <td><%= link_to 'Destroy', report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />
 
